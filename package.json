{
  "name": "gitraffe-update-server",
  "version": "1.0.0",
  "main": "lib/index.js",
  "description": "Lightweight update server for tauri apps",
  "scripts": {
    "test": "xo && jest"
  },
  "license": "MIT",
  "repository": "mikaelkristiansson/gitraffe-update-server",
  "xo": {
    "extends": [
      "prettier"
    ],
    "rules": {
      "camelcase": 0,
      "new-cap": 0,
      "unicorn/no-process-exit": 0,
      "no-await-in-loop": 0,
      "unicorn/import-index": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn test && :",
      "prettier --single-quote --no-semi --write --no-editorconfig",
      "git add"
    ]
  },
  "dependencies": {
    "async-retry": "1.3.3",
    "date-fns": "3.6.0",
    "express-useragent": "1.0.15",
    "finalhandler": "1.2.0",
    "handlebars": "4.7.8",
    "jest": "29.7.0",
    "micro": "10.0.1",
    "ms": "2.1.3",
    "node-fetch-commonjs": "^3.3.2",
    "router": "1.3.8",
    "semver": "7.6.0",
    "stream-to-string": "1.2.1",
    "test-listen": "1.1.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "xo": "0.58.0"
  }
}
